						.col-auto: a(href="#") И
						.col-auto: a(href="#") Й
						.col-auto: a(href="#") К
						.col-auto: a(href="#") Л
						.col-auto: a(href="#") М
						.col-auto: a(href="#") Н
						.col-auto: a(href="#") О
						.col-auto: a(href="#") П
						.col-auto: a(href="#") Р
						.col-auto: a(href="#") С
						.col-auto: a(href="#") Т
						.col-auto: a(href="#") У
