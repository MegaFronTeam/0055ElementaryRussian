			+alphabet
			table
				tr
					th Термин
					th Описание
					th Перевод
				mixin tr(td1, td2, td3)
					tr&attributes(attributes)
						td!= td1
						td!= td2
						td!= td3
				+tr('ада́жио', 'нескл., с.', '慢板  不可数名词')
				+tr('aккомпаниа́тор', '', '伴奏')
				+tr('аккомпани́ровать', 'нсв (-ирую, -ируешь, -ируют)', '伴奏者')
				+tr('саккомпани́ровать', 'св (кому? 3 п. на чём? 6 п.)', '伴奏 动词完成式 （为谁？第三格，用什么？第六格）')
				+tr('', '~ певице на рояле', '用钢琴为女歌手~').default
				+tr().p-0
				+tr('акко́рд', 'финальный ~', '和弦， 终止~')
				+tr('акко́рдовый', '-ая, -ое, -ые; ~ звук', '和弦的')
				+tr('aку́стика', '', '音响，音效')
				+tr('алле́гро', 'нескл., с.', '快板  不可数名词')
				+tr('', '1) в темпе ~ ', '1）~ 的速度').default
				+tr('', '2) сонатное ~', '2）奏鸣曲式的 ~ ').default
				+tr('аллема́нда', '', '阿勒芒德 （舞曲）')
			+alphabet
	// end sDictionary